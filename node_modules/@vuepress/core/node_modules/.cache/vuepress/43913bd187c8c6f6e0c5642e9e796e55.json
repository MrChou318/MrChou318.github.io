D,MAAL,KAAgB,KAAKN,IAAL,CAAUS,MAArC,CAAT;AACH;;AACD,WAAKC,UAAL,GAAkBJ,MAAlB;AACA,UAAIN,IAAI,GAAG,KAAKA,IAAL,CAAUM,MAAV,CAAX;;AANY,iCAOHK,CAPG;AAQRR,QAAAA,UAAU,CAAC,YAAM;AACb,UAAA,MAAI,CAACF,QAAL,IAAiBD,IAAI,CAACW,CAAD,CAArB;;AACA,cAAIA,CAAC,KAAKX,IAAI,CAACS,MAAL,GAAc,CAAxB,EAA2B;AACvBN,YAAAA,UAAU,CAAC,YAAM;AACb,cAAA,MAAI,CAACS,WAAL;AACH,aAFS,EAEP,IAFO,CAAV;AAGH;AACJ,SAPS,EAOPD,CAAC,GAAG,GAPG,CAAV;AARQ;;AAOZ,WAAK,IAAIA,CAAC,GAAG,CAAb,EAAgBA,CAAC,GAAGX,IAAI,CAACS,MAAzB,EAAiCE,CAAC,EAAlC,EAAsC;AAAA,cAA7BA,CAA6B;AASrC;AACJ,KAlBI;AAmBLC,IAAAA,WAnBK,yBAmBU;AAAA;;AACX;AACA,WAAK,IAAID,CAAC,GAAG,CAAb,EAAgBA,CAAC,GAAG,KAAKV,QAAL,CAAcQ,MAAlC,EAA0CE,CAAC,EAA3C,EAA+C;AAC3CR,QAAAA,UAAU,CAAC,YAAM;AACb,UAAA,MAAI,CAACF,QAAL,GAAgB,MAAI,CAACA,QAAL,CAAcY,SAAd,CAAwB,CAAxB,EAA2B,MAAI,CAACZ,QAAL,CAAcQ,MAAd,GAAuB,CAAlD,CAAhB;;AACA,cAAI,MAAI,CAACR,QAAL,CAAcQ,MAAd,KAAyB,CAA7B,EAAgC;AAC5BN,YAAAA,UAAU,CAAC,YAAM;AACb,cAAA,MAAI,CAACC,YAAL;AACH,aAFS,EAEP,IAFO,CAAV;AAGH;AACJ,SAPS,EAOPO,CAAC,GAAG,EAPG,CAAV;AAQH;AACJ;AA/BI;AAnBE,CAAf","sourcesContent":["//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n\nexport default {\n    name: \"my-demo\",\n    data () {\n        return {\n            text: ['今日事，今日毕。', '人之进学在于思，思则能知是与非。', '年年岁岁花相似，岁岁年年人不同。', '春种一粒粟，秋收万颗子。',\n                '锲而不舍，金石可镂。——荀子', '　　锲而不舍，金石可镂。——荀子', '愿你的生命有够多的云翳，造成一个美丽的黄昏。', '为着后来的回忆，小心着意的描你现在的图画。',\n                '要说别人脑子有病，脑子有病的前提是必须有个脑子。', '为中华而努力读书！一包中华好多钱啊！', '如果帅可以当饭吃，那我的帅可以让13亿人吃饱了。', '我的工作啊，价格便宜量又足。',\n                '从小学到大学，唯一不变的就是一颗不想念书的心。', '想咬你一口，可惜我是回民。', '生活不只有眼前的苟且，还有读不懂的诗和到不了的远方。'],\n            pageText: '人之进学在于思，思则能知是与非。'\n        }\n    },\n    mounted () {\n        setTimeout(() => {\n            this.initPageText()\n        }, 1500)\n        // this.$nextTick(function () {\n        //   this.initPageText()\n        // })\n    },\n    methods: {\n        initPageText () {\n            let random = Math.floor(Math.random() * this.text.length)\n            while (this.textRandom === random) {\n                random = Math.floor(Math.random() * this.text.length)\n            }\n            this.textRandom = random\n            let text = this.text[random]\n            for (let i = 0; i < text.length; i++) {\n                setTimeout(() => {\n                    this.pageText += text[i]\n                    if (i === text.length - 1) {\n                        setTimeout(() => {\n                            this.delPageText()\n                        }, 2200)\n                    }\n                }, i * 200)\n            }\n        },\n        delPageText () {\n            // let i = 0\n            for (let i = 0; i < this.pageText.length; i++) {\n                setTimeout(() => {\n                    this.pageText = this.pageText.substring(0, this.pageText.length - 1)\n                    if (this.pageText.length === 0) {\n                        setTimeout(() => {\n                            this.initPageText()\n                        }, 1500)\n                    }\n                }, i * 70)\n            }\n        },\n    }\n}\n"]}]}